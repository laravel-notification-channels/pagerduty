{
    "name": "laravel-notification-channels/pagerduty",
    "description": "A Laravel notification channel for sending PagerDuty events.",
    "keywords": [
        "laravel",
        "laravel-package",
        "notifications",
        "notification-channel",
        "pagerduty",
        "alerting"
    ],
    "homepage": "https://github.com/laravel-notification-channels/pagerduty",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Waite",
            "email": "lwaite@gmail.com",
            "homepage": "https://lukewaite.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6.0|^7.0",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ^8.0|^9.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ^8.0|^9.0"
    },
    "require-dev": {
        "illuminate/queue": "~5.5 || ~6.0 || ~7.0 || ^8.0|^9.0",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PagerDuty\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\PagerDuty\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
